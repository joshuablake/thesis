@article{fogartyBayesian,
  title = {Bayesian Hierarchical Regression on Clearance Rates in the Presence of “Lag” and “Tail” Phases with an Application to Malaria Parasites},
  author = {Fogarty, Colin B. and Fay, Michael P. and Flegg, Jennifer A. and Stepniewska, Kasia and Fairhurst, Rick M. and Small, Dylan S.},
  date = {2015},
  journaltitle = {Biometrics},
  volume = {71},
