@article{quiltyQuarantine,
  title = {Quarantine and Testing Strategies in Contact Tracing for {{SARS-CoV-2}}: A Modelling Study},
  shorttitle = {Quarantine and Testing Strategies in Contact Tracing for {{SARS-CoV-2}}},
  author = {Quilty, Billy J. and Clifford, Samuel and Hellewell, Joel and Russell, Timothy W. and Kucharski, Adam J. and Flasche, Stefan and Edmunds, W. John and Atkins, Katherine E. and Foss, Anna M. and Waterlow, Naomi R. and Abbas, Kaja and Lowe, Rachel and Pearson, Carl A. B. and Funk, Sebastian and Rosello, Alicia and Knight, Gwenan M. and Bosse, Nikos I. and Procter, Simon R. and Gore-Langton, Georgia R. and Showering, Alicia and Munday, James D. and Sherratt, Katharine and Jombart, Thibaut and Nightingale, Emily S. and Liu, Yang and Jarvis, Christopher I. and Medley, Graham and Brady, Oliver and Gibbs, Hamish P. and Simons, David and Williams, Jack and Tully, Damien C. and Flasche, Stefan and Meakin, Sophie R. and Zandvoort, Kevin and Sun, Fiona Y. and Jit, Mark and Klepac, Petra and Quaife, Matthew and Eggo, Rosalind M. and Sandmann, Frank G. and Endo, Akira and Prem, Kiesha and Abbott, Sam and Barnard, Rosanna and Chan, Yung-Wai D. and Auzenbergs, Megan and Gimma, Amy and Villabona-Arenas, C. Julian and Davies, Nicholas G.},
  date = {2021-03-01},
  journaltitle = {The Lancet Public Health},
  shortjournal = {The Lancet Public Health},
  volume = {6},
  number = {3},
  eprint = {33484644},
  eprinttype = {pmid},
  pages = {e175-e183},
  publisher = {{Elsevier}},
  issn = {2468-2667},
  doi = {10/gh5s7c},
  abstract = {{$<$}h2{$>$}Summary{$<$}/h2{$><$}h3{$>$}Background{$<$}/h3{$><$}p{$>$}In most countries, contacts of confirmed COVID-19 cases are asked to quarantine for 14 days after exposure to limit asymptomatic onward transmission. While theoretically effective, this policy places a substantial social and economic burden on both the individual and wider society, which might result in low adherence and reduced policy effectiveness. We aimed to assess the merit of testing contacts to avert onward transmission and to replace or reduce the length of quarantine for uninfected contacts.{$<$}/p{$><$}h3{$>$}Methods{$<$}/h3{$><$}p{$>$}We used an agent-based model to simulate the viral load dynamics of exposed contacts, and their potential for onward transmission in different quarantine and testing strategies. We compared the performance of quarantines of differing durations, testing with either PCR or lateral flow antigen (LFA) tests at the end of quarantine, and daily LFA testing without quarantine, against the current 14-day quarantine strategy. We also investigated the effect of contact tracing delays and adherence to both quarantine and self-isolation on the effectiveness of each strategy.{$<$}/p{$><$}h3{$>$}Findings{$<$}/h3{$><$}p{$>$}Assuming moderate levels of adherence to quarantine and self-isolation, self-isolation on symptom onset alone can prevent 37\% (95\% uncertainty interval [UI] 12–56) of onward transmission potential from secondary cases. 14 days of post-exposure quarantine reduces transmission by 59\% (95\% UI 28–79). Quarantine with release after a negative PCR test 7 days after exposure might avert a similar proportion (54\%, 95\% UI 31–81; risk ratio [RR] 0·94, 95\% UI 0·62–1·24) to that of the 14-day quarantine period, as would quarantine with a negative LFA test 7 days after exposure (50\%, 95\% UI 28–77; RR 0·88, 0·66–1·11) or daily testing without quarantine for 5 days after tracing (50\%, 95\% UI 23–81; RR 0·88, 0·60–1·43) if all tests are returned negative. A stronger effect might be possible if individuals isolate more strictly after a positive test and if contacts can be notified faster.{$<$}/p{$><$}h3{$>$}Interpretation{$<$}/h3{$><$}p{$>$}Testing might allow for a substantial reduction in the length of, or replacement of, quarantine with a small excess in transmission risk. Decreasing test and trace delays and increasing adherence will further increase the effectiveness of these strategies. Further research is required to empirically evaluate the potential costs (increased transmission risk, false reassurance) and benefits (reduction in the burden of quarantine, increased adherence) of such strategies before adoption as policy.{$<$}/p{$><$}h3{$>$}Funding{$<$}/h3{$><$}p{$>$}National Institute for Health Research, UK Research and Innovation, Wellcome Trust, EU Horizon 2021, and the Bill \& Melinda Gates Foundation.{$<$}/p{$>$}},
  langid = {english},
  file = {C\:\\Users\\Joshua\\Documents\\paper_pdfs\\Quilty et al_2021_Quarantine and testing strategies in contact tracing for SARS-CoV-2.pdf;C\:\\Users\\Joshua\\Documents\\paper_pdfs\\Quilty et al_2021_Quarantine and testing strategies in contact tracing for SARS-CoV-3.pdf}
}

