@article{birrellBayesian,
  title = {Bayesian Modeling to Unmask and Predict Influenza {{A}}/{{H1N1pdm}} Dynamics in {{London}}},
  author = {Birrell, Paul J. and Ketsetzis, Georgios and Gay, Nigel J. and Cooper, Ben S. and Presanis, Anne M. and Harris, Ross J. and Charlett, André and Zhang, Xu-Sheng and White, Peter J. and Pebody, Richard G. and De Angelis, Daniela},
