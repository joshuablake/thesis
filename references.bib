@article{ealesAppropriately,
  title = {Appropriately Smoothing Prevalence Data to Inform Estimates of Growth Rate and Reproduction Number},
  author = {Eales, Oliver and Ainslie, Kylie E. C. and Walters, Caroline E. and Wang, Haowei and Atchison, Christina and Ashby, Deborah and Donnelly, Christl A. and Cooke, Graham and Barclay, Wendy and Ward, Helen and Darzi, Ara and Elliott, Paul and Riley, Steven},
  date = {2022-09-01},
  journaltitle = {Epidemics},
  shortjournal = {Epidemics},
  volume = {40},
  pages = {100604},
  issn = {1755-4365},
  doi = {10.1016/j.epidem.2022.100604},
  abstract = {The time-varying reproduction number (Rt) can change rapidly over the course of a pandemic due to changing restrictions, behaviours, and levels of population immunity. Many methods exist that allow the estimation of Rt from case data. However, these are not easily adapted to point prevalence data nor can they infer Rt across periods of missing data. We developed a Bayesian P-spline model suitable for fitting to a wide range of epidemic time-series, including point-prevalence data. We demonstrate the utility of the model by fitting to periodic daily SARS-CoV-2 swab-positivity data in England from the first 7 rounds (May 2020–December 2020) of the REal-time Assessment of Community Transmission-1 (REACT-1) study. Estimates of Rt over the period of two subsequent rounds (6–8 weeks) and single rounds (2–3 weeks) inferred using the Bayesian P-spline model were broadly consistent with estimates from a simple exponential model, with overlapping credible intervals. However, there were sometimes substantial differences in point estimates. The Bayesian P-spline model was further able to infer changes in Rt over shorter periods tracking a temporary increase above one during late-May 2020, a gradual increase in Rt over the summer of 2020 as restrictions were eased, and a reduction in Rt during England’s second national lockdown followed by an increase as the Alpha variant surged. The model is robust against both under-fitting and over-fitting and is able to interpolate between periods of available data; it is a particularly versatile model when growth rate can change over small timescales, as in the current SARS-CoV-2 pandemic. This work highlights the importance of pairing robust methods with representative samples to track pandemics.},
  langid = {english},
  file = {C\:\\Users\\Joshua\\Documents\\paper_pdfs\\Eales et al_2022_Appropriately smoothing prevalence data to inform estimates of growth rate and.pdf;C\:\\Users\\Joshua\\Documents\\paper_pdfs\\Eales et al_2022_Appropriately smoothing prevalence data to inform estimates of growth rate and2.pdf}
}

@article{ealesCharacterising,
