@article{frassoBayesian,
  title = {Bayesian Inference in an Extended {{SEIR}} Model with Nonparametric Disease Transmission Rate: An Application to the {{Ebola}} Epidemic in {{Sierra Leone}}},
  shorttitle = {Bayesian Inference in an Extended {{SEIR}} Model with Nonparametric Disease Transmission Rate},
  author = {Frasso, Gianluca and Lambert, Philippe},
