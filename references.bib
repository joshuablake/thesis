  author = {Manley, Harrison and Park, Josie and Bevan, Luke and Sanchez-Marroquin, Alberto and Danelian, Gabriel and Bayley, Thomas and Bowman, Veronica and Maishman, Thomas and Finnie, Thomas and Charlett, André and Watkins, Nicholas A. and Hutchinson, Johanna and Riley, Steven and Group, Nowcasts Model Contributing and Panovska-Griffiths, Jasmina},
  date = {2023-11-07},
  eprinttype = {medRxiv},
  pages = {2023.02.27.23286501},
  doi = {10.1101/2023.02.27.23286501},
  abstract = {The effective reproduction number R was widely accepted as a key indicator during the early stages of the COVID-19 pandemic. In the UK, the R value published on the UK Government Dashboard has been generated as a combined value from an ensemble of epidemiological models via a collaborative initiative between academia and government. In this paper we outline this collaborative modelling approach and illustrate how, by using an established combination method, a combined R estimate can be generated from an ensemble of epidemiological models. We analyse the R values calculated for the period between April 2021 and December 2021, to show that this R is robust to different model weighting methods and ensemble size, and that using heterogeneous data sources for validation increases its robustness and reduces the biases and limitations associated with a single source of data. We discuss how R can be generated from different data sources and is therefore a good summary indicator of the current dynamics in an epidemic.},
  langid = {english},
  pubstate = {preprint},
  file = {C\:\\Users\\Joshua\\Documents\\paper_pdfs\\Manley et al_2023_Combining models to generate a consensus effective reproduction number R for.pdf;C\:\\Users\\Joshua\\Documents\\paper_pdfs\\Park et al_2023_Combining models to generate a consensus effective reproduction number R for.pdf}
