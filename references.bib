@article{amirthalingamSeroprevalence,
  title = {Seroprevalence of {{SARS-CoV-2}} among {{Blood Donors}} and {{Changes}} after {{Introduction}} of {{Public Health}} and {{Social Measures}}, {{London}}, {{UK}}},
  author = {Amirthalingam, Gayatri and Whitaker, Heather and Brooks, Tim and Brown, Kevin and Hoschler, Katja and Linley, Ezra and Borrow, Ray and Brown, Colin and Watkins, Nick and Roberts, David J. and Solomon, Danielle and Gower, Charlotte M. and family=Waroux, given=Olivier le Polain, prefix=de, useprefix=true and Andrews, Nick J. and Ramsay, Mary E.},
