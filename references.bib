  author = {Nicholson, George and Lehmann, Brieuc and Padellini, Tullia and Pouwels, Koen B. and Jersakova, Radka and Lomax, James and King, Ruairidh E. and Mallon, Ann-Marie and Diggle, Peter J. and Richardson, Sylvia and Blangiardo, Marta and Holmes, Chris},
