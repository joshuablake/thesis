@article{birrellRealtime,
  title = {Real-Time Nowcasting and Forecasting of {{COVID-19}} Dynamics in {{England}}: The First Wave},
  shorttitle = {Real-Time Nowcasting and Forecasting of {{COVID-19}} Dynamics in {{England}}},
  author = {Birrell, Paul and Blake, Joshua and family=Leeuwen, given=Edwin, prefix=van, useprefix=true and Gent, Nick and De Angelis, Daniela},
  date = {2021-07-19},
  journaltitle = {Philosophical Transactions of the Royal Society B: Biological Sciences},
